{
  "name": "vcwebnetworks-nodets",
  "version": "1.0.0",
  "main": "./dist/server/index.js",
  "repository": "git@github.com:vagnercardosoweb/vcwebnetworks-nodets.git",
  "author": "Vagner Cardoso <vagnercardosoweb@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --poll --respawn --transpile-only --ignore-watch './node_modules' './src/server/index.ts'",
    "dev:debug": "cross-env NODE_ENV=development ts-node-dev --inspect=0.0.0.0:9229 --poll --respawn --transpile-only './src/server/index.ts'",
    "dev:docker": "docker-compose -f docker-compose.yml up",
    "start": "node ./dist/server/index.js",
    "build": "tsc",
    "build:docker": "./.docker/scripts/build.sh",
    "lint": "eslint --ext ts ./src",
    "lint:fix": "yarn lint --fix",
    "db:migrate": "yarn build && sequelize db:migrate && rimraf ./dist",
    "db:seed": "yarn build && sequelize db:seed:undo:all && sequelize db:seed:all && rimraf ./dist",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "yarn test --watch -c ./jest-unit.config.ts",
    "test:integration": "yarn test --watch -c ./jest-integration.config.ts",
    "test:staged": "yarn test -o",
    "test:ci": "yarn test --coverage --no-cache",
    "updatelibs": "npm-check -s -u",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/node": "^6.14.1",
    "@sentry/tracing": "^6.14.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-graceful-shutdown": "^3.1.5",
    "i18next": "^21.4.2",
    "i18next-http-middleware": "^3.1.4",
    "ioredis": "^4.28.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "rate-limiter-flexible": "^2.3.6",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "sequelize": "^6.9.0",
    "sequelize-typescript": "^2.1.1",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.28.3",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/response-time": "^2.3.5",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.6",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "npm-check": "^5.9.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.3.0",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
